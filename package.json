{
	"name": "giver",
	"version": "1.0.0",
	"description": "Dependency injection with modern ECMAScript decorators",
	"author": "Palmer McCutcheon <palmermccutcheon@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/palmm/giver.git"
	},
	"keywords": [
		"typescript",
		"dependency injection",
		"inversion of control",
		"ioc",
		"di"
