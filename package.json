	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.1",
		"@jest/globals": "^29.7.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.4",
		"tsup": "^8.1.0",
		"typescript": "^5.4.5"
	},
	"license": "MIT"
